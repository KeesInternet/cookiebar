{% if not isCookie %}
<div class="cookiebar-container">
    <div class="cookiebar">
        <span class="cookiebar-label">{{ label }}</span>
        {{ text }}
        <a href="javascript();" id="true" class="btn btn-primary js-cookiebar-button">{{ accept_button_text }}</a>
        <a href="javascript();" id="false" class="btn btn-danger js-cookiebar-button">{{ decline_button_text }}</a>

    </div>
</div>
{% endif %}